/* CF7 */
.wpcf7-form:before, 
.wpcf7-form:after {
	content: '\0020';
	display: block;
	overflow: hidden;
	visibility: hidden;
	width: 0;
	height: 0
}
.wpcf7-form:after {
	clear: both
}
.wpcf7-form p {
	position: relative;
	display: inline-block;
	width: 100%;
	margin-bottom: 10px
}
.wpcf7-form p:last-of-type {
	float: none !important
}
