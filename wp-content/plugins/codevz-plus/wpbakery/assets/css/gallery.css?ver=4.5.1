	display: none
}
.cz_grid_filters li {
	position: relative;
	display: inline-block;
	color: #a7a7a7;
	margin: 0 0 20px;
	padding: 6px 20px;
	cursor: pointer;
	border-radius: 50px;
	border: 1px solid transparent;
	transition: all .3s ease-in-out
